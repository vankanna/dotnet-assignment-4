@using Northwind.Models
@model IEnumerable<Discount>

<h2 class="mt-3"><i class="fas fa-list-ul"></i>Discounts</h2>
<ul class="list-group">
    @foreach (Discount discount in Model)
    {
        <a class="list-group-item list-group-item-action" asp-action="Index" asp-route-id="@discount.DiscountId">@discount.Title - @discount.Code</a>
    }
</ul>