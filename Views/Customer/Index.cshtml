@using Northwind.Models
@model IEnumerable<Customer>

<h2 class="mt-3"><i class="fas fa-list-ul"></i>customers</h2>
<a class="btn btn-primary btn-lg active" role="button" asp-action="Register" asp-controller="Customer"><i class="fas fa-users"></i> Register New Customer</a>
<ul class="list-group">
    @foreach (Customer customer in Model)
    {
        <li class="list-group-item list-group-item-action">@customer.CompanyName</li>
    }
</ul>